{
  "name": "woop",
  "displayName": "woop",
  "publisher": "chattkush",
  "description": "Project launcher for VS Code",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kush/Woop.git"
  },
  "version": "0.1.5",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "statusBar/context": [
        {
          "command": "woop.dashGUI",
          "when": "true",
          "group": "navigation"
        },
        {
          "command": "woop.view",
          "when": "true",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "woop.view",
        "title": "View Project Dir"
      },
      {
        "command": "woop.setCmd",
        "title": "Set Cmd for the current Project"
      },
      {
        "command": "woop.noti",
        "title": "Notify"
      },
      {
        "command": "woop.dashGUI",
        "title": "dashGUI"
      },
      {
        "command": "woop.dash",
        "title": "dash.dash: Open a Project"
      },
      {
        "command": "woop.setDash",
        "title": "Set Project Dir"
      },
      {
        "command": "woop.removeProjectDir",
        "title": "Remove Project Dir"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
